{
  "name": "azora-os",
  "version": "2.0.0",
  "type": "module",
  "description": "Azora OS - Enterprise-grade AI-powered operating system with cognitive simulation",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:contracts": "npx hardhat compile",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:services": "npm run test --workspaces",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "ci": "npm run lint && npm run type-check && npm run test",
    "start": "./START_AZORA_OS.sh",
    "docs:api": "npm run docs:api --workspaces",
    "clean": "rm -rf dist coverage node_modules/.cache"
  },
  "workspaces": [
    "services/api-gateway",
    "services/services/email-service",
    "services/messaging",
    "services/chamber-of-ghosts"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "private": true,
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/react": "^7.0.0",
    "@types/cors": "^2.8.19",
    "axios": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.0",
    "express": "^5.1.0",
    "framer-motion": "^11.0.0",
    "i18next": "^23.0.0",
    "i18next-browser-languagedetector": "^7.0.0",
    "i18next-http-backend": "^2.0.0",
    "leaflet": "^1.9.0",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.344.0",
    "rate-limiter-flexible": "^8.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^2.0.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.0.0",
    "react-signature-canvas": "^1.0.6",
    "recharts": "^2.0.0",
    "socket.io-client": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^2.0.0",
    "vite": "^4.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-node": "^11.0.0",
    "hardhat": "^2.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "supertest": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
