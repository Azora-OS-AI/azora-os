# AZORA Mint-Mine Engine Environment Variables
# Copy this file to .env and fill in your actual values

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=azora_os
DB_USER=azora
DB_PASSWORD=your_password_here

# Blockchain Configuration
AZORA_RPC_URL=http://localhost:8545
AZORA_CHAIN_ID=1337
AZR_CONTRACT_ADDRESS=0xYourContractAddressHere

# Wallet Configuration (Keep secure!)
MINTER_PRIVATE_KEY=0xYourPrivateKeyHere
MINTER_ADDRESS=0xYourWalletAddressHere

# Security
ALERT_WEBHOOK=https://your-webhook-url.com/alerts

# Optional: Mining Pool API (if using external pools)
# WOOLYPOOLY_API_KEY=your_api_key_if_needed