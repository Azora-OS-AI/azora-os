{
  "dashboard": {
    "id": null,
    "title": "Azora OS Overview",
    "tags": ["azora", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "AI/ML Engine Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_request_duration_seconds_sum{job=\"ai-ml-engine\"}[5m]) / rate(http_request_duration_seconds_count{job=\"ai-ml-engine\"}[5m])",
            "legendFormat": "Response Time"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Service Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"ai-ml-engine|ai-orchestrator|auth-service|api-gateway|email-service|messaging-service|chamber-of-ghosts\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Quantum Microservice Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"quantum-microservice\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "API Gateway Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"api-gateway\"}[5m])",
            "legendFormat": "API Gateway Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Email Service Queue Status",
        "type": "stat",
        "targets": [
          {
            "expr": "http_requests_total{job=\"email-service\", endpoint=\"/api/email/send\"}",
            "legendFormat": "Emails Queued"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Messaging Service Activity",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"messaging-service\", endpoint=\"/api/messages\"}[5m])",
            "legendFormat": "Messages Sent/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Chamber of Ghosts Simulations",
        "type": "stat",
        "targets": [
          {
            "expr": "http_requests_total{job=\"chamber-of-ghosts\", endpoint=\"/api/trigger/past\"} + http_requests_total{job=\"chamber-of-ghosts\", endpoint=\"/api/trigger/present\"} + http_requests_total{job=\"chamber-of-ghosts\", endpoint=\"/api/trigger/future\"}",
            "legendFormat": "Simulations Run"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
