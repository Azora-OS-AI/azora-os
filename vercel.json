{
  "version": 2,
  "name": "azora-onboarding",
  "builds": [
    {
      "src": "services/azora-onboarding/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "services/azora-onboarding/server.ts"
    },
    {
      "src": "/status",
      "dest": "services/azora-onboarding/server.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "services/azora-onboarding/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "services/azora-onboarding/server.ts"
    }
  ],
  "env": {
    "ONBOARDING_PORT": "5500",
    "NODE_ENV": "production"
  }
}
