{
  "version": 2,
  "name": "azora-os",
  "builds": [
    {
      "src": "api/production-server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/production-server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/ui/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
