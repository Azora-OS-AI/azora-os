{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "ignoreCommand": "exit 0",
  "env": {
    "ENABLE_EXPERIMENTAL_COREPACK": "0"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
