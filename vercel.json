{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "env": {
    "ENABLE_EXPERIMENTAL_COREPACK": "0",
    "NODE_VERSION": "20"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
        "Access-Control-Allow-Headers": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
      },
      "continue": true
    },
    {
      "src": "/api/ai-orchestrator/(.*)",
      "dest": "/services/ai-orchestrator/index.js"
    },
    {
      "src": "/api/klipp/(.*)",
      "dest": "/services/klipp-service/index.js"
    },
    {
      "src": "/api/neural/(.*)",
      "dest": "/services/neural-context-engine/index.js"
    },
    {
      "src": "/api/retail/(.*)",
      "dest": "/services/retail-partner-integration/index.js"
    },
    {
      "src": "/api/coldchain/(.*)",
      "dest": "/services/cold-chain-quantum/index.js"
    },
    {
      "src": "/api/safety/(.*)",
      "dest": "/services/universal-safety/index.js"
    },
    {
      "src": "/api/autonomous/(.*)",
      "dest": "/services/autonomous-operations/index.js"
    },
    {
      "src": "/api/tracking/(.*)",
      "dest": "/services/quantum-tracking/index.js"
    },
    {
      "src": "/api/hr-ai/(.*)",
      "dest": "/services/hr-ai-deputy/index.js"
    },
    {
      "src": "/api/onboarding/(.*)",
      "dest": "/services/onboarding-service/index.js"
    },
    {
      "src": "/api/compliance/(.*)",
      "dest": "/services/compliance-service/index.js"
    },
    {
      "src": "/api/traffic/(.*)",
      "dest": "/services/traffic-routing/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
