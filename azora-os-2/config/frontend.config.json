{
  "apiBaseUrl": "https://api.azora-os.com",
  "frontendPort": 3000,
  "enableAnalytics": true,
  "theme": {
    "primaryColor": "#4CAF50",
    "secondaryColor": "#FF5722",
    "fontFamily": "Arial, sans-serif"
  },
  "routes": {
    "dashboard": "/dashboard",
    "login": "/login",
    "signup": "/signup",
    "profile": "/profile",
    "settings": "/settings"
  },
  "features": {
    "enableNotifications": true,
    "enableDarkMode": false,
    "enableMultiLanguageSupport": true
  },
  "apiKeys": {
    "googleMaps": "YOUR_GOOGLE_MAPS_API_KEY",
    "stripe": "YOUR_STRIPE_API_KEY"
  }
}